{
  "name": "jonastorres.dev",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:blog\"  \"npm run mockdb\"",
    "start:blog": "ng serve --project=blog --port 42001",
    "mockdb": "json-server --watch db.json --delay 750 --port 801",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "concurrently \"npm run test:blog\"  \"npm run test:guide-dog\"",
    "test:blog": "ng test blog --code-coverage",
    "test:guide-dog": "ng test guide-dog --code-coverage",
    "coverage": "ng test --no-watch --code-coverage",
    "eslint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js, jsx,ts,tsx,html,css,scss}' --write",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.2.4",
    "@angular/common": "~15.2.4",
    "@angular/compiler": "~15.2.4",
    "@angular/core": "~15.2.4",
    "@angular/forms": "~15.2.4",
    "@angular/platform-browser": "~15.2.4",
    "@angular/platform-browser-dynamic": "~15.2.4",
    "@angular/router": "~15.2.4",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@tinkoff/ng-dompurify": "^3.0.0",
    "chart.js": "^3.9.1",
    "dompurify": "^2.3.6",
    "guid-typescript": "^1.0.9",
    "hamburgers": "^1.2.1",
    "ng2-charts": "^4.0.0",
    "platform": "^1.3.6",
    "primeicons": "^5.0.0",
    "primeng": "^14.0.2",
    "quill": "^1.3.7",
    "rxjs": "~7.5.0",
    "socicon": "^3.0.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "~15.2.4",
    "@types/dompurify": "^2.3.3",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/platform": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.0.0",
    "json-server": "^0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.9.5"
  }
}
